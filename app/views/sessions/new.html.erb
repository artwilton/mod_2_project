<%= form_for @user, url: login_path, method: :post do |f| %>
    <%= flash[:errors] %>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br>
    <%= f.submit "log in" %>
    Or <%= link_to 'sign up', new_user_path %>
<% end %>